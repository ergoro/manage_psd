\begin_inset Float table
placement h
wide false
sideways false
status open

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Detailed TFP summary statistics - Furniture
\end_layout

\end_inset


\begin_inset CommandInset label
LatexCommand label
name "TFP sum stat furniture"

\end_inset


\end_layout

